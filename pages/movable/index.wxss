/* pages/movable/index.wxss */
.page-section {
  padding: 20px;
}

.page-section-title {
  font-size: 28rpx;
  color: #999999;
  margin: 40rpx 0 20rpx;
}

movable-view {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100rpx;
  width: 100rpx;
  background: #1AAD19;
  color: #fff;
}

movable-area {
  height: 400rpx;
  width: 400rpx;
  background-color: #ccc;
  overflow: hidden;
}

.max {
  width: 600rpx;
  height: 600rpx;
  background-color: blue;
}